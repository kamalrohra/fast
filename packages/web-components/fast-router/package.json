{
  "name": "@microsoft/fast-router",
  "description": "A web-components-based router.",
  "sideEffects": false,
  "version": "1.0.0-alpha.26",
  "author": {
    "name": "Microsoft",
    "url": "https://discord.gg/FcSNfg4"
  },
  "homepage": "https://www.fast.design/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast.git",
    "directory": "packages/web-components/fast-router"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/fast/issues/new/choose"
  },
  "main": "dist/esm/index.js",
  "types": "dist/dts/index.d.ts",
  "type": "module",
  "unpkg": "dist/fast-router.min.js",
  "scripts": {
    "clean:dist": "node ../../../build/clean.js dist",
    "doc": "api-extractor run --local",
    "doc:ci": "api-extractor run",
    "build": "yarn build:tsc && yarn doc",
    "build:tsc": "tsc -p ./tsconfig.json",
    "dev": "tsc -p ./tsconfig.json -w",
    "prepare": "yarn clean:dist && yarn build",
    "prettier": "prettier --config ../../../.prettierrc --write \"**/*.ts\"",
    "prettier:diff": "prettier --config ../../../.prettierrc \"**/*.ts\" --list-different",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "test": "yarn eslint && yarn build:tsc && web-test-runner && yarn doc:ci"
  },
  "devDependencies": {
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@microsoft/api-extractor": "7.24.2",
    "@web/test-runner": "0.17.0",
    "@web/test-runner-playwright": "0.10.1",
    "@types/mocha": "^7.0.2",
    "@types/webpack-env": "^1.15.2",
    "esm": "^3.2.25",
    "ignore-loader": "^0.1.2",
    "mocha": "^7.1.2",
    "prettier": "2.8.8",
    "source-map": "^0.7.3",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^7.0.2",
    "ts-node": "^8.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.7.0"
  },
  "dependencies": {
    "@microsoft/fast-element": "^2.0.0-beta.26"
  }
}
